{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    }
  }
}
